SET LINESIZE 500
SET PAGESIZE 500
SPOOL empresarias.TXT

SELECT 
DISTRITOS.DICODIGO,
DISTRITOS.DINOME,
CLIENTES.CCODIGO,
CLIENTES.CTIPO,
CLIPFISICA.CCPF,
CLIENTES.CNOME,
GRUPOS.GCODIGO,
GRUPOS.GNOME
FROM CLIENTES, CLIPFISICA, GRUPOS, DISTRITOS
WHERE	CLIENTES.CCODIGO = CLIPFISICA.CCODIGO AND
	CLIENTES.CGRUPO = GRUPOS.GCODIGO AND
	GRUPOS.GDISTRITO = DISTRITOS.DICODIGO AND
CLIENTES.CTIPO IN ('0','1') AND
DISEMBAIXA IS NULL
ORDER BY DICODIGO;

SPOOL OFF

CLEAR BREAKS
CLEAR COMPUTES

SET FEED   ON
SET VERIFY ON 
SET ECHO   ON
